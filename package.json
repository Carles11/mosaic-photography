{
  "name": "mosaic",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx src/ --fix",
    "format-imports": "eslint --ext .js,.jsx,.ts,.tsx src/ --fix --quiet --rule \"import/order: error\" --rule \"import/no-duplicates: error\"",
    "lint:imports": "eslint --ext .js,.jsx,.ts,.tsx src/ --quiet --rule \"import/order: error\" --rule \"import/no-duplicates: error\"",
    "analyze": "cross-env ANALYZE=true next build",
    "postbuild": "ts-node --project tsconfig.json scripts/generate-image-sitemap.ts && ts-node --project tsconfig.json scripts/generate-collection-sitemap.ts",
    "generate-image-sitemap": "ts-node --project tsconfig.json src/scripts/generate-image-sitemap.ts",
    "update-orientations": "node scripts/update-image-orientations.js",
    "clean": "rimraf .next"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^15.3.1",
    "@next/third-parties": "^15.3.3",
    "@supabase/supabase-js": "^2.49.1",
    "@types/qrcode": "^1.5.5",
    "js-cookie": "^3.0.5",
    "jszip": "^3.10.1",
    "next": "^15.3.5",
    "next-pwa": "^5.6.0",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.4",
    "photoswipe": "^5.4.4",
    "qrcode": "^1.5.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-photoswipe-gallery": "^3.0.2",
    "react-share": "^5.2.2",
    "react-slick": "^0.30.3",
    "react-spinners": "^0.15.0",
    "react-tooltip": "^5.28.0",
    "react-window": "^1.8.11",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/js-cookie": "^3.0.6",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-slick": "^0.23.13",
    "@types/react-window": "^1.8.8",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "eslint-plugin-import": "^2.32.0",
    "node-fetch": "2",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "sharp": "^0.34.3",
    "typescript": "^5"
  },
  "resolutions": {
    "minimatch": "^9.0.3",
    "@types/minimatch": "^5.1.2"
  }
}
