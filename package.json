{
  "name": "mosaic",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "build": "next build",
    "pages:build": "next build && npx @cloudflare/next-on-pages@latest",
    "vercel-build": "NEXT_IGNORE_TSC_ERRORS=1 next build",
    "postbuild": "tsx scripts/generate-image-sitemap.ts && tsx scripts/generate-collection-sitemap.ts",
    "start": "next start",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx src/ --fix",
    "format-imports": "eslint --ext .js,.jsx,.ts,.tsx src/ --fix --quiet --rule \"import/order: error\" --rule \"import/no-duplicates: error\"",
    "lint:imports": "eslint --ext .js,.jsx,.ts,.tsx src/ --quiet --rule \"import/order: error\" --rule \"import/no-duplicates: error\"",
    "analyze": "cross-env ANALYZE=true next build",
    "generate-image-sitemap": "tsx src/scripts/generate-image-sitemap.ts",
    "update-orientations": "node scripts/update-image-orientations.js",
    "clean": "rimraf .next"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^15.5.3",
    "@next/third-parties": "^15.3.3",
    "@supabase/supabase-js": "^2.49.1",
    "@types/qrcode": "^1.5.5",
    "js-cookie": "^3.0.5",
    "jszip": "^3.10.1",
    "keen-slider": "^6.8.6",
    "next": "^15.4.6",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.4",
    "photoswipe": "^5.4.4",
    "prop-types": "^15.8.1",
    "qrcode": "^1.5.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-photoswipe-gallery": "^3.0.2",
    "react-share": "^5.2.2",
    "react-spinners": "^0.15.0",
    "react-tooltip": "^5.28.0",
    "react-window": "^2.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.0.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.1",
    "eslint": "^9",
    "eslint-config-next": "^15.5.3",
    "jest": "^29.7.0",
    "node-fetch": "2",
    "sharp": "^0.34.3",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.3",
    "typescript": "^5"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Edge versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "not IE 11",
    "not dead"
  ]
}
