{
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    }
  ],
  "build": {
    "env": {
      "NODE_OPTIONS": "--experimental-vm-modules",
      "NEXT_IGNORE_TSC_ERRORS": "1"
    }
  }
}
